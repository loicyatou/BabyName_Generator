<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="fsd.week1.todolist1.datamodel.ChartsController">
    <headerText>
        Add an item to the to-do list: --->>>
    </headerText>
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text="Short Description " GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="NameOfLocation" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text="Details:" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <TextArea fx:id="detailsTA" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Location Delivery Date:" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
            <DatePicker fx:id="deadlinePicker" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
